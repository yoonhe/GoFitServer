{
  "parser": "esprima",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
  },
  "env": {
      "commonjs": true,
      "node": true,
      "es6" : true
  },
  "extends": [
    "airbnb-base", "plugin:prettier/recommended"    
  ],
  "plugins": [
    "import", "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error"]
  } 
}
